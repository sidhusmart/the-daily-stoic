{
    "manifest_version": 3,
    "name": "Daily Stoic",
    "description": "Reflect with a daily Stoic quote in every new tab.",
    "version": "1.0",
    "chrome_url_overrides": {
      "newtab": "newtab.html"
    },
    "permissions": [],
    "web_accessible_resources": [
      {
        "resources": ["stoics.json"],
        "matches": ["<all_urls>"]
      }
    ],
    "action": {
      "default_title": "Daily Stoic"
    }
  }